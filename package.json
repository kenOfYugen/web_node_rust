{
  "name": "webrust",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:native": "cargo t --manifest-path native/estimate_pi/Cargo.toml",
    "compile:rust:native": "cargo b --manifest-path native/estimate_pi/Cargo.toml",
    "compile:rust:native:release": "npm run compile:rust:native -- --release",
    "compile:rust:asmjs": "cd native/pi_est_browser/ && cargo b --target=asmjs-unknown-emscripten",
    "compile:rust:asmjs:release": "npm run compile:rust:asmjs -- --release",
    "compile:rust:wasm": "cd native/pi_est_browser/ && cargo b --target=wasm32-unknown-emscripten",
    "compile:rust:wasm:release": "npm run compile:rust:wasm -- --release",
    "compile:pug": "pug -P --out target/web/ src/web/",
    "compile:coffeescript:node": "coffee -b --no-header -o target/node/ -c src/",
    "compile:coffeescript:web": "browserify --debug -t coffeeify --extension=\".coffee\" src/index.coffee > target/web/bundle.js",
    "clean": "rm -rf node_modules/ target/ build/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bindings": "^1.3.0",
    "browserify": "^14.5.0",
    "coffeeify": "^3.0.0",
    "coffeescript": "^2.0.1",
    "node-gyp": "^3.6.2",
    "pug-cli": "^1.0.0-alpha6"
  }
}
